                       List<ConstraintNode> l = new LinkedList<ConstraintNode>(); l.add(a); RESULT = l; 
                       :}
 |  where_constraint_list:a COMMA where_constraint:c {:
                                                     a.add(c); RESULT = a; 
                                                     :};

