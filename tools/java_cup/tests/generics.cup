			if (a.parens != null) parser.die(a.parens.pos);
			RESULT = parser.nf.ModelDecl(parser.pos(a,c,i), fl.flags(), parser.nf.Id(parser.pos(c), c.getIdentifier()), d, f.toAmbConceptNode(), g.typeArgs, g.modelArgs, h, i); 
                                                                                                                                                      :};

