				objtype.addBracket(b.brackets.get(0));
				TypeNode tn = objtype.toType();
				RESULT = parser.nf.New(parser.pos(a), tn, parser.listToTypes(b.brackets.get(1).list), b.brackets.get(1).models, d, f);
