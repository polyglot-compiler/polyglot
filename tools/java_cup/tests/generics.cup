                                                          :}
 |  reftype_or_expr:a DOT LPAREN objtype_or_expr:p RPAREN generic_inst_args_opt:q LPAREN argument_list_opt:c RPAREN:d {:
                                                                                                                     	if (p.prefix == null || p.brackets != null) parser.die(parser.pos(p));
