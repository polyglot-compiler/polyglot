import java.util.function.Function;

public class InstanceMethodReferenceInvalid {
    Integer intF(Integer m) { return m; }

    void test() {
        // Invalid return type
        Function<Integer, String> negation = this::intF;
        // Invalid param type
        Function<String, Integer> negation2 = this::intF;
        negation.apply(3);
    }
}
